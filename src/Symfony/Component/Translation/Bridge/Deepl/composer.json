{
    "name": "symfony/deepl-translation-provider",
    "type": "symfony-translation-bridge",
    "description": "Symfony Deepl Translation Provider Bridge",
    "keywords": ["deepl", "translation", "provider"],
    "homepage": "https://symfony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Raffaele Carelle",
            "email": "raffaele.carelle@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/config": "^6.4|^7.0",
        "symfony/http-client": "^6.4|^7.0",
        "symfony/translation": "^6.4|^7.0",
        "deeplcom/deepl-php": "^v1.9",
        "nyholm/psr7": "^1.8@dev"
    },
    "autoload": {
        "psr-4": { "Symfony\\Component\\Translation\\Bridge\\Deepl\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0@dev"
    }
}
